# IBM watsonx Configuration
# Required for watsonx provider support

# IBM Cloud API Key
# Get from: https://cloud.ibm.com/iam/apikeys
WATSONX_API_KEY=your-api-key-here

# watsonx Project ID
# Get from your watsonx project settings
WATSONX_PROJECT_ID=your-project-id-here

# watsonx Service URL (optional, defaults to UK)
# Options: https://eu-gb.ml.cloud.ibm.com, https://us-south.ml.cloud.ibm.com, etc.
WATSONX_URL=https://eu-gb.ml.cloud.ibm.com
# Set to 'false' to disable watsonx provider
WATSONX_ENABLED=true

# OpenRouter Configuration
OPENROUTER_API_KEY=your-api-key-here
# Set to 'false' to disable OpenRouter provider
OPENROUTER_ENABLED=true

# Google Gemini Configuration
GEMINI_API_KEY=your-api-key-here
# Set to 'false' to disable Gemini provider
GEMINI_ENABLED=true

# Ollama Configuration
# Set to 'false' to explicitly disable Ollama provider (optional, defaults to true)
OLLAMA_ENABLED=true

